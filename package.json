{
  "name": "snaked",
  "version": "1.0.0",
  "description": "react snake example",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "startw": "npm run buildw",
    "build": "webpack -d && cp src/index.html  dist/index.html && webpack-dev-server --content-base src/ --inline --hot",
    "buildw": "webpack -d && copy src\\index.html  dist\\index.html && webpack-dev-server --content-base src/ --inline --hot",
    "build:prod": "webpack -p && cp src/index.html dist/index.html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "reactjs"
  ],
