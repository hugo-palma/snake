    "babel-loader": "^8.3.0",
    "mobx": "^5.15.4",
    "mobx-react": "^6.1.8",
    "mobx-state-tree": "^3.15.0",
    "moment": "^2.24.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
